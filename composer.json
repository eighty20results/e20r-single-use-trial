  "minimum-stability": "stable",
  "version": "2.3",
  "license": "GPL-2.0-or-later",
  "authors": [
    {
      "name": "Thomas Sjolshagen",
      "email": "thomas@eighty20results.com",
      "homepage": "https://eighty20results.com/",
      "role": "Developer"
    }
  ],
  "require-dev": {
    "antecedent/patchwork": "^2.1",
    "brain/faker": "dev-master",
    "brain/monkey": "^2.3",
    "codeception/module-asserts": "^1.0",
    "codeception/module-cli": "^1.0",
    "codeception/module-db": "^1.0",
    "codeception/module-filesystem": "^1.0",
    "codeception/module-phpbrowser": "^1.0",
    "codeception/module-webdriver": "^1.0",
    "codeception/util-universalframework": "^1.0",
    "fzaninotto/faker": "v1.9",
    "lucatume/wp-browser": "^2.6",
    "mockery/mockery": "^1.4",
    "php-parallel-lint/php-parallel-lint": "^0.9",
    "phpmd/phpmd": "^2.8",
    "phpro/grumphp": "^0.21",
    "phpunit/phpunit": "^9.3",
    "sebastian/phpcpd": "^6.0",
    "wp-coding-standards/wpcs": "^2.3"
  },
  "require": {
    "wp-cli/wp-cli": "^v2.4"
  },
  "scripts": {
    "post-install-cmd": [
      "inc/bin/phpcs --config-set installed_paths inc/wp-coding-standards/wpcs"
